{
   "env": {
      "browser": true,
      "node": true,
      "es6": true
   },
   "parser": "babel-eslint",
   "rules": {
      "linebreak-style": "off",
      "no-console": 1,
      "quotes": 1,
      "indent": 0,
      "no-unused-vars": 1,
      "eol-last": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "import/prefer-default-export": 0,
      "class-methods-use-this": 0
   },
   "extends": "eslint-config-airbnb-base"
}